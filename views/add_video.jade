extends layout
block content
  .container
    form(action="/add_video", method="post", enctype="multipart/form-data")
      input(type="file", name="video")
      button(type='submit')
    form(class="by-link")
      span.label Enter URL to video
      input(type="text",class="link")
      button(type="submit") Add URL

block scripts
  != js('add_video')
